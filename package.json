{
  "name": "@gvempire/www",
  "version": "0.0.3",
  "scripts": {
    "start": "next",
    "build": "next build",
    "serve": "next start",
    "type-check": "tsc",
    "format": "prettier --write **/*/*.{jsx,js,ts,tsx,md,mdx}",
    "lint": "eslint ./src --fix",
    "test": "jest -c jest.config.js",
    "test:watch": "jest -c jest.config.js --watch",
    "test:coverage": "jest -c jest.config.js --coverage",
    "deploy": "npm-run-all -l -s deploy:live",
    "deploy:live": "now deploy",
    "deploy:prod": "now deploy --prod",
    "changelog": "git log --all --graph --decorate --oneline > CHANGELOG; git add .; git commit -m \"update changelog\"; yarn version --patch; git push --follow-tags; echo \"Released $npm_package_version\"",
    "predeploy": "npm-run-all -l -s format lint test"
  },
  "dependencies": {
    "@emotion/babel-preset-css-prop": "^10.0.27",
    "@emotion/core": "^10.0.28",
    "@mdx-js/loader": "^1.5.8",
    "@mdx-js/react": "^1.5.8",
    "@next/mdx": "^9.3.2",
    "@sendgrid/mail": "^6.5.5",
    "dotenv": "^8.2.0",
    "emotion-reset": "^2.0.7",
    "emotion-theming": "^10.0.27",
    "framer-motion": "^1.10.3",
    "isomorphic-unfetch": "3.0.0",
    "next": "latest",
    "prism-react-renderer": "^1.0.2",
    "react": "^16.x.x",
    "react-accessible-accordion": "^3.0.1",
    "react-dom": "^16.12.0",
    "react-hook-form": "^5.1.3",
    "react-icons": "^3.9.0",
    "sitemap": "^6.1.0"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.9.0",
    "@testing-library/dom": "^6.12.2",
    "@testing-library/jest-dom": "^5.1.1",
    "@testing-library/react": "^9.4.0",
    "@testing-library/user-event": "^7.1.2",
    "@types/node": "^12.12.21",
    "@types/react": "^16.9.16",
    "@types/react-dom": "^16.9.4",
    "@typescript-eslint/parser": "^2.25.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-jest": "^23.8.2",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.19.0",
    "jest": "^25.1.0",
    "now": "^17.0.4",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.1",
    "typescript": "latest"
  },
  "license": "ISC"
}
